    "bootstrap": "^4.1.3",
    "cors": "^2.8.5",
    "css-loader": "^0.28.0",
    "dsteem": "^0.10.1",
    "enzyme": "^2.8.1",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "immutable": "^3.8.2",
    "mocha": "^3.2.0",
    "moment": "^2.22.2",
    "mongoose": "^5.3.2",
    "nginx-conf": "^1.5.0",
    "node-sass": "^4.5.2",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.12",
    "react-hot-loader": "^4.3.11",
    "react-markdown": "^4.0.3",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-rte": "^0.16.1",
    "react-scroll": "^1.7.10",
    "react-scroll-up": "^1.3.3",
    "react-sticky": "^6.0.3",
